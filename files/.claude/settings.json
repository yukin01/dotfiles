{
  "env": {
    "DISABLE_AUTOUPDATER": "1"
  },
  "permissions": {
    "allow": [
      "Bash(gh pr view:*)",
      "Bash(gh pr diff:*)",
      "Bash(gh pr list:*)",
      "Bash(gh issue view:*)",
      "Bash(gh issue list:*)",
      "Bash(gh project item-list:*)",
      "Bash(git rev-parse:*)",
      "Bash(git push -u origin:*)",
      "Bash(tail)",
      "Bash(terraform fmt:*)",
      "WebFetch"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "plan"
  },
  "hooks": {
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Funk.aiff"
          }
        ]
      }
    ]
  },
  "language": "Japanese",
  "plansDirectory": "./.local/plans"
}
